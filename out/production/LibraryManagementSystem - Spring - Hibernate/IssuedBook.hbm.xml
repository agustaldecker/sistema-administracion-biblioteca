<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name = "IssuedBook" table = "issued_books">

        <meta attribute = "class-description">
            This class contains the librarians detail.
        </meta>

        <id name = "id" type = "int" column = "ID"/>


        <property name = "studentId" column = "Student_ID" type = "int"/>
        <property name = "callNo" column = "Call_No" type = "int"/>
        <property name = "name" column = "Student_Name" type = "string"/>
        <property name = "contact" column = "Student_Contact" type = "string"/>

    </class>
</hibernate-mapping>